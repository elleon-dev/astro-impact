{
  "hosting": [
    {
      "target": "api",
      "rewrites": [
        {
          "source": "/**",
          "function": "api"
        }
      ],
      "headers": []
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "predeploy": [
      "yarn functions build"
    ],
    "source": "apps/functions"
  }
}
